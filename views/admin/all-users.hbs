<div style="margin:30px 30px ;" >
    <h3 class="text-center" style="margin-top:50px ; margin-bottom: 40px;">All users</h3>
    <table class="table border w-75 ">
        <thead class="thead-dark ">
            <tr>
                <th scope="col">User name</th>
                <th scope="col">Email</th>
                
            </tr>
        </thead>
        <tbody class="bg-light">
            {{#each allUsers}}
            <tr>

                <td>{{this.Username}}</td>
                <td>{{this.Email}}</td>
               
            </tr>
            {{/each}}
        </tbody>

    </table>
</div>